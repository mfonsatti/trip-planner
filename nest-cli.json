{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/trip-api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/trip-api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/trip-api-gateway",
  "projects": {
    "contracts": {
      "type": "library",
      "root": "libs/contracts",
      "entryFile": "index",
      "sourceRoot": "libs/contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/contracts/tsconfig.lib.json"
      }
    },
    "trip-api-gateway": {
      "type": "application",
      "root": "apps/trip-api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/trip-api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/trip-api-gateway/tsconfig.app.json"
      }
    },
    "trip-crud": {
      "type": "application",
      "root": "apps/trip-crud",
      "entryFile": "main",
      "sourceRoot": "apps/trip-crud/src",
      "compilerOptions": {
        "tsConfigPath": "apps/trip-crud/tsconfig.app.json"
      }
    },
    "trip-manager": {
      "type": "application",
      "root": "apps/trip-manager",
      "entryFile": "main",
      "sourceRoot": "apps/trip-manager/src",
      "compilerOptions": {
        "tsConfigPath": "apps/trip-manager/tsconfig.app.json"
      }
    },
    "trip-search": {
      "type": "application",
      "root": "apps/trip-search",
      "entryFile": "main",
      "sourceRoot": "apps/trip-search/src",
      "compilerOptions": {
        "tsConfigPath": "apps/trip-search/tsconfig.app.json"
      }
    }
  }
}