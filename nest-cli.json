{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/trip-planner/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/trip-planner/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/trip-planner",
  "projects": {
    "trip-search": {
      "type": "application",
      "root": "apps/trip-search",
      "entryFile": "main",
      "sourceRoot": "apps/trip-search/src",
      "compilerOptions": {
        "tsConfigPath": "apps/trip-search/tsconfig.app.json"
      }
    }
  }
}